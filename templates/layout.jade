doctype html
block vars
  - var bodyclass = null;
html(lang='en')
  head
    block head
      meta(charset='utf-8')
      meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
      meta(name='viewport', content='width=device-width')
      title
        block title
          = locals.name
      // begin :og block
      block ogMeta
      
      include partials/styles
  body(class=bodyclass)
    
    // Begin: Content
    include partials/aside
    // Begin: Content
    main(role=main) 
      // Begin: header
      include partials/head
      // Begin: Content
      block content
      // Begin: footer
      include partials/footer
      // Begin: scripts
      include partials/scripts
            
        

    
